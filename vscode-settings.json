{
  "workbench.iconTheme": "material-icon-theme",
  "window.zoomLevel": 0,
  "editor.fontSize": 17,
  "workbench.colorTheme": "GitHub Dark",
  "eslint.enable": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "vetur.format.defaultFormatter.html": "prettier",
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[plaintext]": {
    "editor.defaultFormatter": "raynigon.nginx-formatter"
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "discord.workspaceElapsedTime": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "discord.largeImage": "‚ú® Editing a {LANG} file",
  "discord.largeImageIdling": "‚è± Idling",
  "editor.suggest.showStatusBar": true,
  "liveServer.settings.donotVerifyTags": true,
  "discord.detailsEditing": "üíª Editing {file_name} in {workspace}",
  "discord.lowerDetailsEditing": "üìÑ line: {current_line}/{total_lines} - Branch: {git_branch} - {file_size}",
  "editor.fontFamily": "Fira Code",
  "scm.providerCountBadge": "auto",
  "material-icon-theme.showWelcomeMessage": false,
  "prettier.printWidth": 100,
  "files.associations": {
    ".env": "dotenv"
  },
 
}
