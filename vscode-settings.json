{
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontSize": 17,
  "workbench.colorTheme": "GitHub Dark",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.codeActionsOnSave.mode": "problems",
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[xml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "discord.largeImage": "‚ú® Editing a {LANG} file",
  "discord.largeImageIdling": "‚è± Idling",
  "editor.suggest.showStatusBar": true,
  "liveServer.settings.donotVerifyTags": true,
  "discord.detailsEditing": "üíª Editing {file_name} in {workspace}",
  "discord.lowerDetailsEditing": "üìÑ line: {current_line}/{total_lines} - Branch: {git_branch} - {file_size}",
  "editor.fontFamily": "Fira Code",
  "scm.providerCountBadge": "auto",
  "material-icon-theme.showWelcomeMessage": false,
  "prettier.printWidth": 100,
  "files.associations": {
    ".env": "dotenv"
  },
  "cSpell.userWords": [
    "Hoverable",
    "URL's",
    "cadv",
    "caspertheghost",
    "citizenfx",
    "nocheck",
    "prefetch",
    "zyndicate"
  ],
  "editor.linkedEditing": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "extensions.ignoreRecommendations": true,
  "javascript.preferences.quoteStyle": "double",
  "typescript.preferences.quoteStyle": "double",
  "workbench.welcomePage.hiddenCategories": ["Setup", "Beginner"],
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "redhat.telemetry.enabled": false,
  "todo-tree.tree.showScanModeButton": false,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.eol": "\n",
  "remote.WSL.server.connectThroughLocalhost": true
}
